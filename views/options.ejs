      <div id="test">
        <h1>Options</h1>
      </div>
    </header>

    <%- include('./partials/nav.ejs') %>

    <main>
      <form id="companyForm">
        <div><input type="text" id="insertCompany" name="insertCompany" placeholder="New Company |" /></div>
        <div><input type="submit" value="Add" /></div>
      </form>

      <form id="removeCompanyForm">
        <div>
          <select id="removeCompanyDropdown">
            <option value="">Select Company</option>
          </select>
        </div>
        <div><input type="submit" value="Remove" /></div>
      </form>

      <label for="multipleInputForms">Use Multiple Input Forms</label><br />
      <input type="checkbox" id="multipleInputForms" value="Use multiple input forms" />
    </main>

    <footer></footer>
  </body>
</html>
