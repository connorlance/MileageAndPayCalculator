<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Date</th>
      <th>Company</th>
      <th>Mileage Start</th>
      <th>Mileage End</th>
      <th>Pay</th>
    </tr>
  </thead>
  <tbody>
    <% data.forEach(row => { %>
    <tr>
      <td><%= row.ID %></td>
      <td><%= new Date(row.date).toLocaleDateString() %></td>
      <td><%= row.company %></td>
      <td><%= row.mileage_start %></td>
      <td><%= row.mileage_end %></td>
      <td><%= row.pay %></td>
    </tr>
    <% }); %>
  </tbody>
</table>
